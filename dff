[1mdiff --git a/Palindrome/app/src/androidTest/java/com/solomon/eyoel/palindrome/ExampleInstrumentedTest.java b/Palindrome/app/src/androidTest/java/com/solomon/eyoel/palindrome/ExampleInstrumentedTest.java[m
