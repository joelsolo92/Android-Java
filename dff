[1m+++ b/GradeCalculator/app/src/main/java/com/solomon/eyoel/gradecalculator/CalculateGrade.java[m
[36m@@ -0,0 +1,140 @@[m
[32m+[m[32mpackage com.solomon.eyoel.gradecalculator;[m
[32m+[m
[32m+[m[32mimport android.support.v7.app.AppCompatActivity;[m
[32m+[m[32mimport android.os.Bundle;[m
[32m+[m[32mimport android.view.View;[m
[32m+[m[32mimport android.widget.Button;[m
[32m+[m[32mimport android.widget.EditText;[m
[32m+[m[32mimport android.widget.TextView;[m
[32m+[m[32mimport android.widget.Toast;[m
[32m+[m
[32m+[m[32mpublic class CalculateGrade extends AppCompatActivity {[m
[32m+[m
[32m+[m[32m    // Global Variables[m
[32m+[m[32m    TextView tvCalculatedGrade;[m
[32m+[m[32m    Button btnClearAll, btnCalculate;[m
[32m+[m[32m    EditText etAttendance, etProject1,etProject2, etProject3, etMidterm,etFinal;[m
[32m+[m[32m    double dblAttendance, dblProject1, dblProject2, dblProject3, dblMidterm, dblFinal;[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    protected void onCreate(Bundle savedInstanceState) {[m
[32m+[m[32m        super.onCreate(savedInstanceState);[m
[32m+[m[32m        setContentView(R.layout.activity_calculate_grade);[m
[32m+[m
[32m+[m
[32m+[m[32m        // Assign ID's to TextViews, EditText's and Buttons[m
[32m+[m[32m        //////////////////////////////////////////////////////////////////////////////////////////////////////[m
[32m+[m
[32m+[m[32m        btnClearAll = (Button) findViewById(R.id.btn_clear_all);[m
[32m+[m[32m        btnCalculate = (Button) findViewById(R.id.btn_calculate);[m
[32m+[m[32m        etAttendance = (EditText) findViewById(R.id.et_attendance);[m
[32m+[m[32m        etProject1 = (EditText) findViewById(R.id.et_project_1);[m
[32m+[m[32m        etProject2 = (EditText) findViewById(R.id.et_project_2);[m
[32m+[m[32m        etProject3 = (EditText) findViewById(R.id.et_project_3);[m
[32m+[m[32m        etMidterm = (EditText) findViewById(R.id.et_midterm);[m
[32m+[m[32m        etFinal = (EditText) findViewById(R.id.et_final);[m
[32m+[m[32m        tvCalculatedGrade = (TextView) findViewById(R.id.tv_calculated_grade);[m
[32m+[m
[32m+[m
[32m+[m[32m        // Event handlers for the two buttons[m
[32m+[m[32m        ///////////////////////////////////////////////////////////////////////////////////////////////////////////[m
[32m+[m[32m        btnClearAll.setOnClickListener(new View.OnClickListener() {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void onClick(View v) {[m
[32m+[m[32m                clearTextFields();[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        btnCalculate.setOnClickListener(new View.OnClickListener() {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void onClick(View v) {[m
[32m+[m[32m                calculateGrades();[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m        ///////////////////////////////////////////////////////////////////////////////////////////////////////////[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Calculates the user inputted grades and assigns a letter grade[m
[32m+[m[32m    public void calculateGrades() {[m
[32m+[m
[32m+[m[32m        // The validateInput method returns true if the user has entered acceptable info.[m
[32m+[m[32m        boolean isValidInput = validateInput();[m
[32m+[m
[32m+[m[32m        if (isValidInput) {[m
[32m+[m[32m            double dblFinalGrade = dblAttendance + (dblProject1 * 0.15) + (dblProject2 * 0.15) +[m
[32m+[m[32m                                   (dblProject3 * 0.15) + (dblMidterm * 0.20) + (dblFinal * 0.25);[m
[32m+[m
[32m+[m[32m            if (dblFinalGrade >= 94) { tvCalculatedGrade.setText("A"); }[m
[32m+[m[32m            else if (dblFinalGrade >= 90 && dblFinalGrade < 94) { tvCalculatedGrade.setText("A-"); }[m
[32m+[m[32m            else if (dblFinalGrade >= 87 && dblFinalGrade < 90) { tvCalculatedGrade.setText("B+"); }[m
[32m+[m[32m            else if (dblFinalGrade >= 83 && dblFinalGrade < 87) { tvCalculatedGrade.setText("B"); }[m
[32m+[m[32m            else if (dblFinalGrade >= 80 && dblFinalGrade < 83) { tvCalculatedGrade.setText("B-"); }[m
[32m+[m[32m            else if (dblFinalGrade >= 77 && dblFinalGrade < 80) { tvCalculatedGrade.setText("C+"); }[m
[32m+[m[32m            else if (dblFinalGrade >= 70 && dblFinalGrade < 77) { tvCalculatedGrade.setText("C"); }[m
[32m+[m[32m            else if (dblFinalGrade >= 60 && dblFinalGrade < 70) { tvCalculatedGrade.setText("D"); }[m
[32m+[m[32m            else { tvCalculatedGrade.setText("F"); }[m
[32m+[m
[32m+[m[32m            tvCalculatedGrade.setTextSize(24);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    ////////////////////////////////////////////////////////////////////////////////////////////////////////////[m
[32m+[m[32m    // Method which validates the user info[m
[32m+[m[32m    public boolean validateInput() {[m
[32m+[m
[32m+[m[32m        // Assign the entered values to string variables[m
[32m+[m[32m        String attendance, project1, project2, project3, midterm, finalExam;[m
[32m+[m
[32m+[m[32m        attendance = etAttendance.getText().toString();[m
[32m+[m[32m        project1 = etProject1.getText().toString();[m
[32m+[m[32m        project2 = etProject2.getText().toString();[m
[32m+[m[32m        project3 = etProject3.getText().toString();[m
[32m+[m[32m        midterm = etMidterm.getText().toString();[m
[32m+[m[32m        finalExam = etFinal.getText().toString();[m
[32m+[m
[32m+[m
[32m+[m[32m        if(attendance.isEmpty() || project1.isEmpty() || project2.isEmpty() ||  // if no input was entered[m
[32m+[m[32m                project3.isEmpty() || midterm.isEmpty() || finalExam.isEmpty())[m
[32m+[m[32m        {[m
[32m+[m[32m            Toast.makeText(this, "Please fill in all the fields", Toast.LENGTH_SHORT).show();[m
[32m+[m[32m            return false;[m
[32m+[m[32m        }[m
[32m+[m[32m        else if (Double.parseDouble(attendance) > 10) {  // if values for attendance field > 10[m
[32m+[m[32m            Toast.makeText(this, "Attendance should be between 0-10", Toast.LENGTH_SHORT).show();[m
[32m+[m[32m            return false;[m
[32m+[m[32m        }[m
[32m+[m[32m        else[m
[32m+[m
[32m+[m[32m            // convert string values to doubles after validation.[m
[32m+[m[32m            dblAttendance = Double.parseDouble(String.format(attendance, "%.2f"));[m
[32m+[m[32m            dblProject1 = Double.parseDouble(String.format(project1, "%.2f"));[m
[32m+[m[32m            dblProject2 = Double.parseDouble(String.format(project2, "%.2f"));[m
[32m+[m[32m            dblProject3 = Double.parseDouble(String.format(project3, "%.2f"));[m
[32m+[m[32m            dblMidterm = Double.parseDouble(String.format(midterm, "%.2f"));[m
[32m+[m[32m            dblFinal = Double.parseDouble(String.format(finalExam, "%.2f"));[m
[32m+[m
[32m+[m[32m            // check if the values entered are greater than max possible score (100)[m
[32m+[m[32m            if (dblAttendance > 100 || dblProject1 > 100 || dblProject2 > 100 ||[m
[32m+[m[32m                dblProject3 > 100 || dblMidterm > 100 || dblFinal > 100 )[m
[32m+[m[32m            {[m
[32m+[m[32m                Toast.makeText(this, "Please enter values between 0-100", Toast.LENGTH_SHORT).show();[m
[32m+[m[32m                return false;[m
[32m+[m[32m            }[m
[32m+[m[32m            else[m
[32m+[m[32m                return true;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // reset textfields to emptu state[m
[32m+[m[32m    public void clearTextFields() {[m
[32m+[m
[32m+[m[32m        etAttendance.setText("");[m
[32m+[m[32m        etProject1.setText("");[m
[32m+[m[32m        etProject2.setText("");[m
[32m+[m[32m        etProject3.setText("");[m
[32m+[m[32m        etMidterm.setText("");[m
[32m+[m[32m        etFinal.setText("");[m
[32m+[m[32m        tvCalculatedGrade.setText("");[m
[32m+[m
[32m+[m[32m        Toast.makeText(this, "All fields cleared!", Toast.LENGTH_SHORT).show();[m
[32m+[m[32m    }[m
