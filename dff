[32m+[m[32mpackage com.solomon.eyoel.geoquiz;[m
[32m+[m
[32m+[m[32mimport android.content.Context;[m
[32m+[m[32mimport android.content.Intent;[m
[32m+[m[32mimport android.support.v7.app.AppCompatActivity;[m
[32m+[m[32mimport android.os.Bundle;[m
[32m+[m[32mimport android.view.View;[m
[32m+[m[32mimport android.widget.Button;[m
[32m+[m[32mimport android.widget.TextView;[m
[32m+[m
[32m+[m
[32m+[m[32mpublic class CheatActivity extends AppCompatActivity {[m
[32m+[m
[32m+[m[32m    private static final String EXTRA_ANSWER_IS_TRUE = "com.solomon.eyoel.geoquiz.answer_is_true";[m
[32m+[m
[32m+[m[32m    private boolean mAnswerIsTrue;[m
[32m+[m[32m    private TextView mAnswerTextView;[m
[32m+[m[32m    private Button mShowAnswerButton;[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    protected void onCreate(Bundle savedInstanceState) {[m
[32m+[m[32m        super.onCreate(savedInstanceState);[m
[32m+[m[32m        setContentView(R.layout.activity_cheat);[m
[32m+[m
[32m+[m[32m        mAnswerIsTrue = getIntent().getBooleanExtra(EXTRA_ANSWER_IS_TRUE, false);[m
[32m+[m
[32m+[m[32m        mAnswerTextView = (TextView) findViewById(R.id.answer_text_view);[m
[32m+[m[32m        mShowAnswerButton = (Button) findViewById(R.id.show_answer_button);[m
[32m+[m
[32m+[m[32m        mShowAnswerButton.setOnClickListener(new View.OnClickListener() {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void onClick(View v) {[m
[32m+[m[32m                if (mAnswerIsTrue) {[m
[32m+[m[32m                    mAnswerTextView.setText(R.string.true_button);[m
[32m+[m[32m                }[m
[32m+[m[32m                else {[m
[32m+[m[32m                    mAnswerTextView.setText(R.string.false_button);[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static Intent newIntent(Context packageContext, boolean answerIsTrue) {[m
[32m+[m[32m        Intent intent = new Intent(packageContext, CheatActivity.class);[m
[32m+[m[32m        intent.putExtra(EXTRA_ANSWER_IS_TRUE, answerIsTrue);[m
[32m+[m[32m        return intent;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
