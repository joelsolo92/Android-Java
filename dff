[32m+[m[32m        setContentView(R.layout.activity_quiz);[m
[32m+[m
[32m+[m[32m        if (savedInstanceState != null) {[m
[32m+[m[32m            mCurrentIndex = savedInstanceState.getInt(KEY_INDEX, 0);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        mTrueButton = (Button) findViewById(R.id.true_button);[m
[32m+[m[32m        mFalseButton = (Button) findViewById(R.id.false_button);[m
[32m+[m[32m        mNextButton = (Button)findViewById(R.id.next_button);[m
[32m+[m[32m        mQuestionTextView = (TextView) findViewById(R.id.question_text_view) ;[m
[32m+[m[32m        mCheatButton = (Button) findViewById(R.id.cheat_button);[m
[32m+[m
[32m+[m[32m        int question = mQuestionBank[mCurrentIndex].getTextResId();[m
[32m+[m[32m        mQuestionTextView.setText(question);[m
[32m+[m
[32m+[m
[32m+[m[32m        mTrueButton.setOnClickListener(new View.OnClickListener() {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void onClick(View v) {[m
[32m+[m[32m                checkAnswer(true);[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        mFalseButton.setOnClickListener(new View.OnClickListener() {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void onClick(View v) {[m
[32m+[m[32m                checkAnswer(false);[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        mCheatButton.setOnClickListener(new View.OnClickListener() {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void onClick(View v) {[m
[32m+[m[32m           // Intent intent = new Intent(QuizActivity.this, CheatActivity.class);[m
[32m+[m[32m                // startActivity(intent);[m
[32m+[m[32m                boolean answerIsTrue = mQuestionBank[mCurrentIndex].isAnswerTrue();[m
[32m+[m[32m                Intent intent = CheatActivity.newIntent(QuizActivity.this,answerIsTrue);[m
[32m+[m[32m                startActivity(intent);[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        mNextButton.setOnClickListener(new View.OnClickListener() {[m
[32m+[m[32m            @Override[m
[32m+[m[32m            public void onClick(View v) {[m
[32m+[m[32m                mCurrentIndex = (mCurrentIndex + 1) % mQuestionBank.length;[m
[32m+[m[32m                updateQuestion();[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void updateQuestion() {[m
[32m+[m[32m        int question = mQuestionBank[mCurrentIndex].getTextResId();[m
[32m+[m[32m        mQuestionTextView.setText(question);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void checkAnswer (boolean userPressedTrue) {[m
[32m+[m[32m        int messageResId = 0;[m
[32m+[m[32m        boolean answerIsTrue = mQuestionBank[mCurrentIndex].isAnswerTrue();[m
[32m+[m
[32m+[m[32m        if (userPressedTrue == answerIsTrue)[m
[32m+[m[32m        {[m
[32m+[m[32m            messageResId = R.string.correct_toast;[m
[32m+[m[32m        }[m
[32m+[m[32m        else[m
[32m+[m[32m        {[m
[32m+[m[32m            messageResId = R.string.incorrect_toast;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        Toast.makeText(QuizActivity.this, messageResId, Toast.LENGTH_SHORT).show();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onSaveInstanceState(Bundle savedInstanceState) {[m
[32m+[m[32m        super.onSaveInstanceState(savedInstanceState);[m
[32m+[m[32m        Log.i(TAG, "onSaveInstanceState");[m
[32m+[m[32m        savedInstanceState.putInt(KEY_INDEX, mCurrentIndex);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onStart() {[m
[32m+[m[32m        super.onStart();[m
[32m+[m[32m        Log.d(TAG, "onStart() called");[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onResume() {[m
[32m+[m[32m        super.onResume();[m
[32m+[m[32m        Log.d(TAG, "onResume() called");[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onPause() {[m
[32m+[m[32m        super.onPause();[m
[32m+[m[32m        Log.d(TAG, "onPause() called");[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void onStop() {[m
[32m+[m[32m        super.onStop();[m
[32m+[m[32m        Log.d(TAG, "onStop() called");[m
[32m+[m[32m    }[m
[32m+[m
